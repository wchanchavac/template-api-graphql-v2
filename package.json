{
  "name": "template-backend-graphql-v2",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "imports": {
    "#auth": "./src/auth/index.js",
    "#auth/*": "./src/auth/*.js",
    "#config/*": "./src/config/*.js",
    "#database": "./src/database/index.js",
    "#database/*": "./src/database/*.js",
    "#graphql/*": "./src/graphql/*.js",
    "#loaders": "./src/loaders/index.js",
    "#loaders/*": "./src/loaders/*.js",
    "#models/*": "./src/database/models/*.js",
    "#resolvers": "./src/graphql/resolvers/index.js",
    "#resolvers/*": "./src/graphql/resolvers/*.js",
    "#shared/*": "./src/shared/*.js",
    "#types": "./src/graphql/types/index.js",
    "#types/*": "./src/graphql/types/*.js",
    "#utils/*": "./src/graphql/utils/*.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node --env-file=.env --watch ./api/index.js",
    "prettier": "prettier --write .",
    "generate:secret": "node ./src/shared/generate-secrets.js",
    "generate:project": "bunx @satelite/engineer-cli@latest build && bunx run prettier",
    "db:seed:all": "node --env-file=.env ./node_modules/.bin/sequelize-cli db:seed:all",
    "db:seed:specific": "node --env-file=.env ./node_modules/.bin/sequelize-cli db:seed --seed ",
    "db:seed:undo:all": "node --env-file=.env ./node_modules/.bin/sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@as-integrations/next": "^4.0.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.1",
    "argon2": "^0.44.0",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "es-toolkit": "^1.39.10",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "jose": "^6.0.12",
    "oracledb": "^6.9.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@graphql-eslint/eslint-plugin": "^4.4.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "inflection": "^3.0.2",
    "prettier": "3.6.2",
    "sequelize-cli": "^6.6.3"
  }
}
