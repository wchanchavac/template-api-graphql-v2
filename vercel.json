{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": false,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node",
      "includeFiles": ["src/graphql/types/*.graphql", "public/index.html"]
    }
  },
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
